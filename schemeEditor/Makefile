# Don't edit this file! Instead edit `premake.lua` then rerun `make`

ifndef CONFIG
  CONFIG=Release
endif

# if multiple archs are defined turn off automated dependency generation
DEPFLAGS := $(if $(word 2, $(TARGET_ARCH)), , -MMD)

ifeq ($(CONFIG),Release)
  BINDIR := bin/gcc
  LIBDIR := ../../build
  OBJDIR := .objsu
  OUTDIR := bin/gcc
  CPPFLAGS := $(DEPFLAGS) -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "NDEBUG" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_lib/mswu"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -O3 -Wall -fno-strict-aliasing
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -mwindows -s -L"$(WXWIN)/lib/gcc_lib" -L../../lib/gcc_lib -lwxmsw28u_wxsf -lwxmsw28u -lwxexpat -lwxjpeg -lwxpng -lwxtiff -lwxregexu -lwinmm -lrpcrt4 -lkernel32 -luser32 -lgdi32 -lwinspool -lcomdlg32 -ladvapi32 -lshell32 -lole32 -loleaut32 -luuid -lcomctl32 -lwsock32 -lodbc32
  LDDEPS := ../../lib/gcc_lib/libwxmsw28u_wxsf.a
  RESFLAGS := -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "NDEBUG" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_lib/mswu"
  TARGET := wxSFDemo.exe
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

ifeq ($(CONFIG),Debug)
  BINDIR := bin/gcc
  LIBDIR := ../../build
  OBJDIR := .objsud
  OUTDIR := bin/gcc
  CPPFLAGS := $(DEPFLAGS) -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_lib/mswud"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -g -Wall -O0
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -mwindows -L"$(WXWIN)/lib/gcc_lib" -L../../lib/gcc_lib -lwxmsw28ud_wxsf -lwxmsw28ud -lwxexpatd -lwxjpegd -lwxpngd -lwxtiffd -lwxregexud -lwinmm -lrpcrt4 -lkernel32 -luser32 -lgdi32 -lwinspool -lcomdlg32 -ladvapi32 -lshell32 -lole32 -loleaut32 -luuid -lcomctl32 -lwsock32 -lodbc32
  LDDEPS := ../../lib/gcc_lib/libwxmsw28ud_wxsf.a
  RESFLAGS := -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_lib/mswud"
  TARGET := wxSFDemod.exe
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

OBJECTS := \
	$(OBJDIR)/FrameCanvas.o \
	$(OBJDIR)/GUI.o \
	$(OBJDIR)/MainApp.o \
	$(OBJDIR)/MainFrame.o \
	$(OBJDIR)/wx_pch.o \

RESOURCES := \
	$(OBJDIR)/wxSF.res \

MKDIR_TYPE := msdos
CMD := $(subst \,\\,$(ComSpec)$(COMSPEC))
ifeq (,$(CMD))
  MKDIR_TYPE := posix
endif
ifeq (/bin,$(findstring /bin,$(SHELL)))
  MKDIR_TYPE := posix
endif
ifeq ($(MKDIR_TYPE),posix)
  CMD_MKBINDIR := mkdir -p $(BINDIR)
  CMD_MKLIBDIR := mkdir -p $(LIBDIR)
  CMD_MKOUTDIR := mkdir -p $(OUTDIR)
  CMD_MKOBJDIR := mkdir -p $(OBJDIR)
else
  CMD_MKBINDIR := $(CMD) /c if not exist $(subst /,\\,$(BINDIR)) mkdir $(subst /,\\,$(BINDIR))
  CMD_MKLIBDIR := $(CMD) /c if not exist $(subst /,\\,$(LIBDIR)) mkdir $(subst /,\\,$(LIBDIR))
  CMD_MKOUTDIR := $(CMD) /c if not exist $(subst /,\\,$(OUTDIR)) mkdir $(subst /,\\,$(OUTDIR))
  CMD_MKOBJDIR := $(CMD) /c if not exist $(subst /,\\,$(OBJDIR)) mkdir $(subst /,\\,$(OBJDIR))
endif

.PHONY: clean

$(OUTDIR)/$(TARGET): $(OBJECTS) $(LDDEPS) $(RESOURCES)
	@echo Linking wxSFDemo
	-@$(CMD_MKBINDIR)
	-@$(CMD_MKLIBDIR)
	-@$(CMD_MKOUTDIR)
	@$(BLDCMD)

clean:
	@echo Cleaning wxSFDemo
ifeq ($(MKDIR_TYPE),posix)
	-@rm -f $(OUTDIR)/$(TARGET)
	-@rm -rf $(OBJDIR)
else
	-@if exist $(subst /,\,$(OUTDIR)/$(TARGET)) del /q $(subst /,\,$(OUTDIR)/$(TARGET))
	-@if exist $(subst /,\,$(OBJDIR)) del /q $(subst /,\,$(OBJDIR))
	-@if exist $(subst /,\,$(OBJDIR)) rmdir /s /q $(subst /,\,$(OBJDIR))
endif

$(OBJDIR)/FrameCanvas.o: FrameCanvas.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o "$@" -c "$<"

$(OBJDIR)/GUI.o: GUI.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o "$@" -c "$<"

$(OBJDIR)/MainApp.o: MainApp.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o "$@" -c "$<"

$(OBJDIR)/MainFrame.o: MainFrame.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o "$@" -c "$<"

$(OBJDIR)/wx_pch.o: wx_pch.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o "$@" -c "$<"

$(OBJDIR)/wxSF.res: wxSF.rc
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@windres $< -O coff -o "$@" $(RESFLAGS)

-include $(OBJECTS:%.o=%.d)

